{
  "name": "@neox-api/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "_set:env": "node set-env.js",
    "_docker:ops": "node docker-start-env.js",
    "_docker:test": "npm run _set:env test && npm run _docker:ops test",
    "_docker:prod:app": "npm run _set:env production && npm run _docker:ops production",
    "build:prod:app": "npm run _set:env production && NX_NO_CLOUD=true NODE_ENV=production nx build neox-api",
    "check:lint-all": "NX_NO_CLOUD=true nx run-many --all --target=lint",
    "test:e2e": "npm run _set:env test && NX_NO_CLOUD=true NODE_ENV=test nx e2e neox-api-e2e",
    "test:watch:e2e": "npm run test:e2e --watch",
    "test:integration": "npm run _set:env test && NX_NO_CLOUD=true NODE_ENV=test nx int neox-api-int-test",
    "test:watch:int": "npm run test:integration --watch",
    "test:unit": "npm run _set:env test && NX_NO_CLOUD=true NODE_ENV=test nx run-many --target=test --all --passWithNoTests",
    "test:watch:unit": "npm run test:unit --watch",
    "start:dev:app": "npm run _set:env development && NX_NO_CLOUD=true NODE_ENV=development nx run neox-api:serve:development",
    "start:prod:app": "npm run build:prod:app; NODE_ENV=production node dist/apps/neox-api/main.js",
    "start:test:app": "npm run _set:env test && NX_NO_CLOUD=true NODE_ENV=test nx run neox-api:serve:test",
    "start:docker:test:run-all": "npm run _docker:test 'run-all'",
    "start:docker:test:unit": "npm run _docker:test 'npm run test:unit'",
    "start:docker:test:e2e": "npm run _docker:test 'npm run test:e2e'",
    "start:docker:test:int": "npm run _docker:test 'npm run test:int'",
    "start:docker:test:app": "npm run _docker:test",
    "start:docker:dev:app": "npm run _set:env development && npm run _docker:ops development",
    "start:docker:dev:lint": "npm run _docker:prod:app 'npm run check:lint-all'",
    "start:docker:prod:app": "npm run _set:env production && npm run _docker:ops production",
    "migration:generate": "NX_NO_CLOUD=true nx run db:build",
    "migration:run": "NX_NO_CLOUD=true nx run db:migration:run",
    "migration:revert": "NX_NO_CLOUD=true nx run db:migration:revert",
    "migration:show": "NX_NO_CLOUD=true nx run db:migration:show",
    "schema:log": "NX_NO_CLOUD=true nx run db:schema:log",
    "schema:sync": "NX_NO_CLOUD=true nx run db:schema:sync"
  },
  "dependencies": {
    "@fastify/swagger": "^8.14.0",
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/platform-fastify": "^10.3.3",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/typeorm": "^10.0.2",
    "@nx/devkit": "18.0.8",
    "axios": "1.6.7",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crypto-js": "^4.2.0",
    "nestjs-pino": "^3.5.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "pino-http": "^8.6.1",
    "pino-pretty": "10.3.1",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "tslib": "2.6.2",
    "typeorm": "0.3.20",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@fastify/helmet": "11.1.1",
    "@fastify/static": "6.12.0",
    "@nestjs/cli": "10.3.2",
    "@nestjs/schematics": "10.1.1",
    "@nestjs/testing": "10.3.3",
    "@nrwl/cli": "15.9.3",
    "@nx/eslint": "18.0.8",
    "@nx/eslint-plugin": "18.0.8",
    "@nx/jest": "18.0.8",
    "@nx/js": "18.0.8",
    "@nx/nest": "18.0.8",
    "@nx/node": "18.0.8",
    "@nx/web": "18.0.8",
    "@nx/webpack": "18.0.8",
    "@nx/workspace": "18.0.8",
    "@swc-node/register": "1.8.0",
    "@swc/core": "~1.4.2",
    "@swc/helpers": "~0.5.2",
    "@types/bcrypt": "^5.0.2",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.12",
    "@types/node": "18.19.17",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "~8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "29.7.0",
    "jest-environment-node": "^29.7.0",
    "madge": "^6.1.0",
    "nx": "18.0.8",
    "prettier": "^3.2.5",
    "run-script-webpack-plugin": "0.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.1.3",
    "webpack-cli": "^5.1.4"
  }
}
