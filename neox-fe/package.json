    "start:dev:app": "npm run _set:env development && nx serve",
    "build:prod": "npm run _set:env production && nx build --no-cloud",
    "start:docker:test:run-all": "npm run _docker:test 'run-all'",
    "start:docker:dev:app": "npm run _set:env development && npm run _docker:ops development",
    "start:docker:prod": "npm run _set:env production && npm run _docker:ops production"
